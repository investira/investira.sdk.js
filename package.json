{
    "name": "investira.sdk",
    "version": "1.0.0",
    "author": "Investira",
    "description": "Investira SDK",
    "main": "index.js",
    "files": [
        "LICENSE",
        "README.md",
        "build-info.json",
        "index.js",
        "lib/"
    ],
    "type": "version",
    "registry": true,
    "raw": "investira.sdk@1.0.0",
    "escapedName": "investira.sdk",
    "rawSpec": "1.0.0",
    "saveSpec": null,
    "fetchSpec": "1.0.0",
    "homepage": "https://investira.com.br/",
    "keywords": [
        "investira.sdk"
    ],
    "_requiredBy": [
        "/"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/investira/investira.sdk.git"
    },
    "scripts": {
        "start": "nodemon ./lib/tests",
        "test": "nodemon --inspect-brk ./lib/tests/test_model",
        "test_format": "nodemon --inspect-brk ./lib/tests/test_formats"
    },
    "publishConfig": {
        "registry": "http://my-internal-registry.local"
    },
    "dependencies": {
        "axios": "^0.18.0"
    },
    "devDependencies": {
        "tape": "^4.10.1"
    },
    "jshintConfig": {
        "curly": true,
        "eqeqeq": true,
        "immed": true,
        "latedef": false,
        "newcap": false,
        "noarg": true,
        "sub": true,
        "undef": true,
        "boss": true,
        "eqnull": true,
        "strict": false,
        "scripturl": true,
        "evil": true,
        "globals": {
            "location": true,
            "printStackTrace": false
        },
        "browser": true,
        "node": true
    },
    "nodemonConfig": {
        "ignore": [
            ".git",
            "node_modules/**/node_modules"
        ],
        "delay": "0"
    },
    "license": "GPL-3.0-only"
}
