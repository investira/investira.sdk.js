{
    "name": "investira.sdk",
    "version": "2.0.4",
    "author": "Investira",
    "description": "Investira SDK",
    "main": "index.js",
    "type": "version",
    "registry": true,
    "raw": "investira.sdk@2.0.4",
    "escapedName": "investira.sdk",
    "rawSpec": "2.0.4",
    "saveSpec": null,
    "fetchSpec": "2.0.4",
    "homepage": "https://investira.com.br/",
    "keywords": [
        "investira.sdk",
        "investira"
    ],
    "_requiredBy": [
        "/"
    ],
    "repository": {
        "url": "git://github.com/investira/investira.sdk.js.git"
    },
    "bugs": {
        "url": "https://github.com/investira/investira.sdk/issues"
    },
    "scripts": {
        "start": "node ./lib/tests",
        "_test": "nodemon --inspect-brk ./_tests/test"
    },
    "dependencies": {
        "axios": "0.18.1",
        "deep-diff": "^1.0.2",
        "moment": "2.24.0",
        "investira.data": "^1.1.9"
    },
    "devDependencies": {
        "tape": "^4.13.2"
    },
    "jshintConfig": {
        "curly": true,
        "eqeqeq": false,
        "latedef": false,
        "undef": false,
        "varstmt": true,
        "funcscope": true,
        "supernew": true,
        "boss": true,
        "eqnull": true,
        "strict": false,
        "scripturl": true,
        "maxdepth": 4,
        "maxparams": 5,
        "esversion": 9,
        "globals": {
            "esversion": 9,
            "location": true,
            "printStackTrace": false,
            "gLog": true,
            "gRC": true
        },
        "browser": true,
        "node": true
    },
    "nodemonConfig": {
        "ignore": [
            ".git",
            "node_modules/**/node_modules"
        ],
        "delay": "0"
    },
    "prettier": {
        "singleQuote": true,
        "jsxBracketSameLine": true,
        "bracketSpacing": true,
        "trailingComma": "none",
        "tabWidth": 4,
        "arrowParens": "avoid",
        "printWidth": 120
    },
    "license": "GPL-3.0-only",
    "directories": {
        "lib": "lib"
    }
}
